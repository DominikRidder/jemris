<Parameters FOVx="128" FOVy="128" FOVz="1" GradMaxAmpl="2" GradRiseTime="" GradSlewRate="10" Name="P" Nx="64" Ny="34" Nz="1" TE="10" TR="500">
   <ConcatSequence Name="C" Observe="" Repetitions="1">
      <ATOMICSEQUENCE Name="A1">
         <HARDRFPULSE Axis="RF" Duration="0.1" FlipAngle="90" InitialPhase="a1*(a1+1)*50" Name="P1" Observe="C,Counter/C,Repetitions/P,FOVx"/>
      </ATOMICSEQUENCE>
      <ConcatSequence Name="nTE" Observe="P,Ny" Repetitions="a1"><ATOMICSEQUENCE Inclination="" Name="A2" Observe="nTE,Counter" RotAngle="a1*180* (3-sqrt(5))/2">
         <TRAPGRADPULSE Area="-a1/2" Axis="GX" Name="P2" Observe="P3,Area"/>
      </ATOMICSEQUENCE>
      <ATOMICSEQUENCE Name="A3" Observe="A2,RotAngle" RotAngle="a1">
         <TRAPGRADPULSE ADCs="a2" Axis="GX" FlatTopArea="2*a1" FlatTopTime="2" Name="P3" Observe="P,KMAXx/P,Nx/P,FOVx" PhaseLock="1"/>
      </ATOMICSEQUENCE>
      <ATOMICSEQUENCE Name="A4" Observe="A2,RotAngle" RotAngle="a1">
         <TRAPGRADPULSE Area="-a1/2" Axis="GX" Name="P4" Observe="P3,Area" Vector=""/>
      </ATOMICSEQUENCE>
      </ConcatSequence><DELAYATOMICSEQUENCE Delay="a1" DelayType="B2E" Name="D1" Observe="P,TR" StartSeq="A1"/>
   </ConcatSequence>
</Parameters>
