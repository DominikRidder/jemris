<Parameters FOVx="256" FOVy="256" FOVz="256" GradMaxAmpl="2" GradSlewRate="10" Name="P" Nx="16" Ny="16" Nz="16" TE="50" TR="100">
   <ConcatSequence Name="C1" Observe="P,Nz" Repetitions="a1">
      <ATOMICSEQUENCE Name="A1">
         <HARDRFPULSE Duration="0.1" FlipAngle="90" InitialDelay="" InitialPhase="a1*(a1+1)*50" Name="P1" Observe="C1,Counter"/><CONSTANTGRADPULSE Amplitude="a2-a3*a4" Axis="GZ" Duration="a1" Name="P0" Observe="P1,Duration/P,KMAXz/C1,Counter/P,DKz"/></ATOMICSEQUENCE>
      <ATOMICSEQUENCE Name="A2">
         <TRAPGRADPULSE Area="-0.5*abs(a1)" Axis="GX" Name="P2" Observe="P4,Area"/>
         <TRAPGRADPULSE Area="a1" Axis="GY" Name="P3" Observe="P,KMAXy"/>
      </ATOMICSEQUENCE>
      <DELAYATOMICSEQUENCE Delay="a1" DelayType="C2C" Name="D1" Observe="P,TE" StartSeq="A1" StopSeq="C2"/>
      <ConcatSequence Name="C2" Observe="P,Ny" Repetitions="a1">
         <ATOMICSEQUENCE Name="A3">
            <TRAPGRADPULSE ADCs="a3" Axis="GX" FlatTopArea="2*a1*(-1)^a2" FlatTopTime="1" Name="P4" Observe="P,KMAXx/C2,Counter/P,Nx" PhaseLock="1"/>
         </ATOMICSEQUENCE>
         <ATOMICSEQUENCE Name="A4">
            <TRAPGRADPULSE Area="ite(1+a2,a3,0,-a1)" Axis="GY" Name="P5" Observe="P,DKy/C2,Counter/C2,Repetitions" PhaseLock="1"/>
         </ATOMICSEQUENCE>
      </ConcatSequence>
      <DELAYATOMICSEQUENCE Delay="a1" DelayType="B2E" Name="D2" Observe="P,TR" StartSeq="A1"/>
   </ConcatSequence>
</Parameters>
