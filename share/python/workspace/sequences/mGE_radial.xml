<Parameters FOVx="256" FOVy="256" FOVz="1" GradMaxAmpl="2" GradSlewRate="10" Name="P" Nx="128" Ny="204" Nz="1" TE="5" TR="10000">
   <ConcatSequence Name="C" Observe="P,Ny" Repetitions="a1">
      <ATOMICSEQUENCE Name="EX">
         <HARDRFPULSE Axis="RF" Duration="0.1" FlipAngle="90" InitialPhase="" Name="P1" Observe="C,Counter/C,Repetitions/P,FOVx"/>
      </ATOMICSEQUENCE>
      <ConcatSequence Aux1="" Name="TE" Observe="" Repetitions="1"><ATOMICSEQUENCE Name="A2" Observe="A3,RotAngle" RotAngle="a1" Vector="">
         <TRAPGRADPULSE Area="-abs(a1/2)" Axis="GX" Name="P2" Observe="P3,Area"/>
      </ATOMICSEQUENCE>
      <DELAYATOMICSEQUENCE Delay="a1/2" DelayType="B2C" Name="D2" Observe="P,TE" StartSeq="A2" StopSeq="A3"/><ATOMICSEQUENCE Aux1="" Name="A3" Observe="C,Counter/TE,Counter/TE,Repetitions/C,Repetitions" RotAngle="(a1*a3+a2)*180*(3-sqrt(5))">
         <TRAPGRADPULSE ADCs="a2" Area="" Axis="GX" FlatTopArea="2*a1" FlatTopTime="2" Frequency="" Name="P3" Observe="P,KMAXx/P,Nx" PhaseLock="1"/>
      </ATOMICSEQUENCE>
      <DELAYATOMICSEQUENCE Delay="a1/2" DelayType="C2E" Name="D3" Observe="P,TE" StartSeq="A3" StopSeq="A4"/><ATOMICSEQUENCE Name="A4" Observe="A3,RotAngle" RotAngle="a1">
         <TRAPGRADPULSE Area="-a1/2" Axis="GX" Name="P4" Observe="P3,Area" Vector=""/>
      </ATOMICSEQUENCE>
      </ConcatSequence><DELAYATOMICSEQUENCE Delay="a1" DelayType="B2E" Name="D1" Observe="P,TR" StartSeq="A1"/>
   </ConcatSequence>
</Parameters>
