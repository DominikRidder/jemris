<Parameters FOVx="128" FOVy="128" FOVz="1" Name="P" Nx="32" Ny="32" Nz="1" TE="10" TR="500">
   <ConcatSequence Name="C" Observe="" Repetitions="33"><ATOMICSEQUENCE Name="A1">
         <HARDRFPULSE Axis="RF" Duration="0.1" FlipAngle="90" InitialPhase="a1*(a1+1)*50" Name="P1" Observe="C,Counter/C,Repetitions/P,FOVx"/>
      </ATOMICSEQUENCE>
      <ATOMICSEQUENCE Aux1="" Aux2="" Aux3="" Azimut="a1" Inclination="a2" Name="A2" Observe="A2,Azimut/A2,Inclination/A2,RotAngle" RotAngle="a3">
         <TRAPGRADPULSE Area="-a1/2" Axis="GX" Name="P2" Observe="P3,Area"/>
      <TRAPGRADPULSE Area="-a1/2" Axis="GZ" Duration="" MaxAmpl="1" Name="P2z" Observe="P3z,Area" SlewRate="1"/></ATOMICSEQUENCE>
      <ATOMICSEQUENCE Aux1="(2*a1-a2)/(a2)*a3" Azimut="" Inclination="" Name="A3" Observe="C,Counter/C,Repetitions" RotAngle="a1*180*(3-sqrt(5))">
         <TRAPGRADPULSE ADCs="a2" Axis="GX" FlatTopArea="2*a3*sqrt((1 - abs((2*a3-a2)/a2))^2)" FlatTopTime="8" Name="P3" Observe="P,KMAXx/P,Nx/C,Counter/C,Repetitions" PhaseLock="1"/>
      <TRAPGRADPULSE ADCs="" Area="" Asymmetric="" Axis="GZ" FlatTopArea="(2*a1-a2)/(a2)*a3" FlatTopTime="8" MaxAmpl="" Name="P3z" Observe="P3,Area" SlewRate=""/></ATOMICSEQUENCE>
      <ATOMICSEQUENCE Azimut="a1" Inclination="a2" Name="A4" Observe="A2,Azimut/A2,Inclination/A2,RotAngle" RotAngle="a3">
         <TRAPGRADPULSE Area="-a1/2" Axis="GX" Name="P4" Observe="P3,Area" Vector=""/>
      <TRAPGRADPULSE Area="-a1/2" Axis="GZ" MaxAmpl="1" Name="P4z" Observe="P3z,Area" SlewRate="1"/></ATOMICSEQUENCE>
      <DELAYATOMICSEQUENCE Delay="a1" DelayType="B2E" Name="D1" Observe="P,TR" StartSeq="A1"/>
   </ConcatSequence></Parameters>
