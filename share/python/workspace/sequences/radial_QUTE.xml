<Parameters FOVx="128" FOVy="128" FOVz="1" Name="P" Nx="32" Ny="32" Nz="1" TE="10" TR="500">
   <ConcatSequence Name="C" Observe="P,Ny" Repetitions="a1">
      <ATOMICSEQUENCE Name="A1">
         <HARDRFPULSE Axis="RF" Duration="0.1" FlipAngle="90" InitialPhase="a1*(a1+1)*50" Name="P1" Observe="C,Counter/C,Repetitions/P,FOVx"/>
      </ATOMICSEQUENCE>
      <ATOMICSEQUENCE Name="A2" Observe="C,Counter/C,Repetitions" RotAngle="180*a1/a2">
         <TRAPGRADPULSE Area="-abs(a1/2)" Axis="GX" Name="P2" Observe="P3,Area"/>
      </ATOMICSEQUENCE>
      <ConcatSequence Name="nTE" Repetitions="1"><ATOMICSEQUENCE Name="A3" Observe="C,Counter/C,Repetitions" RotAngle="180*a1/a2">
         <TRAPGRADPULSE ADCs="a2" Axis="GX" FlatTopArea="2*a1*(-1)^(a3)" FlatTopTime="8" Name="P3" Observe="P,KMAXx/P,Nx/nTE,Counter" PhaseLock="1"/>
      </ATOMICSEQUENCE>
      </ConcatSequence><ATOMICSEQUENCE Name="A4" Observe="C,Counter/C,Repetitions" RotAngle="180*a1/a2">
         <TRAPGRADPULSE Area="abs(a1/2)*(-1)^(a2)" Axis="GX" Name="P4" Observe="P3,Area/nTE,Repetitions" Vector=""/>
      </ATOMICSEQUENCE>
      <DELAYATOMICSEQUENCE Delay="a1" DelayType="B2E" Name="D1" Observe="P,TR" StartSeq="A1"/>
   </ConcatSequence>
</Parameters>
